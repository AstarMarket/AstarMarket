{
  "name": "demo-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "next dev",
    "tcm:watch": "tcm -p 'src/**/*.module.css' -w",
    "dev": "concurrently \"npm:serve\" \"npm:tcm:watch\" -c blue,green",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc",
    "format": "prettier --write ."
  },
  "dependencies": {
    "daisyui": "2.11.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "0.6.1"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.42",
    "autoprefixer": "10.4.4",
    "concurrently": "7.0.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "postcss": "8.4.12",
    "prettier": "2.6.0",
    "tailwindcss": "3.0.23",
    "typed-css-modules": "0.7.0",
    "typescript": "4.6.2"
  }
}
